    <bpmn:serviceTask id="Activity_0i817zw" name="Fetch API token" zeebe:modelerTemplate="io.camunda.connectors.HttpJson.v2" zeebe:modelerTemplateVersion="8" zeebe:modelerTemplateIcon="data:image/svg+xml;base64,">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="io.camunda:http-json:1" retries="3" />
        <zeebe:ioMapping>
          <zeebe:input source="0" target="writeTimeoutInSeconds" />
          <zeebe:input source="noAuth" target="authentication.type" />
          <zeebe:input source="POST" target="method" />
          <zeebe:input source="https://dummyjson.com/auth/login" target="url" />
          <zeebe:input source="={ &#34;Content-Type&#34;: &#34;application/json&#34; }" target="headers" />
          <zeebe:input source="20" target="connectionTimeoutInSeconds" />
          <zeebe:input source="20" target="readTimeoutInSeconds" />
          <zeebe:input source="={ &#10;  &#34;username&#34;: &#34;emilys&#34;,&#10;  &#34;password&#34;: &#34;emilyspass&#34;&#10;}" target="body" />
        </zeebe:ioMapping>
        <zeebe:taskHeaders>
          <zeebe:header key="resultExpression" value="={ token: body.accessToken }" />
          <zeebe:header key="retryBackoff" value="PT0S" />
        </zeebe:taskHeaders>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_1ro51ky</bpmn:incoming>
      <bpmn:outgoing>Flow_0wa50xv</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:serviceTask id="Activity_0joo3rp" name="Fetch product information" zeebe:modelerTemplate="io.camunda.connectors.HttpJson.v2" zeebe:modelerTemplateVersion="8" zeebe:modelerTemplateIcon="data:image/svg+xml;base64,">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="io.camunda:http-json:1" retries="3" />
        <zeebe:ioMapping>
          <zeebe:input source="bearer" target="authentication.type" />
          <zeebe:input source="=token" target="authentication.token" />
          <zeebe:input source="GET" target="method" />
          <zeebe:input source="=&#34;https://dummyjson.com/auth/products/&#34; + productId" target="url" />
          <zeebe:input source="20" target="connectionTimeoutInSeconds" />
          <zeebe:input source="20" target="readTimeoutInSeconds" />
        </zeebe:ioMapping>
        <zeebe:taskHeaders>
          <zeebe:header key="resultExpression" value="={&#10;  &#34;productName&#34;: body.title,&#10;  &#34;productDescription&#34;: body.description,&#10;  &#34;productPrice&#34;: body.price&#10;}" />
          <zeebe:header key="retryBackoff" value="PT0S" />
        </zeebe:taskHeaders>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_0wa50xv</bpmn:incoming>
      <bpmn:outgoing>Flow_0tdpuam</bpmn:outgoing>
    </bpmn:serviceTask>
